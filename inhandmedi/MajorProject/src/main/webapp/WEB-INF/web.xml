<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AuthorizePaymentServle</servlet-name>
  	<display-name>AuthorizePaymentServle</display-name>
  	<description></description>
  	<servlet-class>AuthorizePaymentServle</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReviewPaymentServlet</servlet-name>
  	<display-name>ReviewPaymentServlet</display-name>
  	<description></description>
  	<servlet-class>ReviewPaymentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ExecutePaymentServlet</servlet-name>
  	<display-name>ExecutePaymentServlet</display-name>
  	<description></description>
  	<servlet-class>ExecutePaymentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AuthorizePaymentServle</servlet-name>
  	<url-pattern>/authorize_payment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewPaymentServlet</servlet-name>
  	<url-pattern>/review_payment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ExecutePaymentServlet</servlet-name>
  	<url-pattern>/execute_payment</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>  
   <welcome-file>Index.html</welcome-file>  
   <welcome-file>Homepage.jsp</welcome-file>  
  </welcome-file-list>  
</web-app>
