        </ownedParameter>
        <language>java</language>
        <body>List&lt;Links> links = approvedPayment.getLinks();&#xD;
 String approvalLink = null;&#xD;
  &#xD;
 for (Links link : links) {&#xD;
     if (link.getRel().equalsIgnoreCase(&quot;approval_url&quot;)) {&#xD;
         approvalLink = link.getHref();&#xD;
         break;&#xD;
     }&#xD;
 }      &#xD;
  &#xD;
 return approvalLink;&#xD;
</body>
      </ownedBehavior>
