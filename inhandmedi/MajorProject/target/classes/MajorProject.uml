     requestPayment.setIntent(&quot;authorize&quot;);&#xD;
 &#xD;
     APIContext apiContext = new APIContext(CLIENT_ID, CLIENT_SECRET, MODE);&#xD;
 &#xD;
     Payment approvedPayment = requestPayment.create(apiContext);&#xD;
 &#xD;
     return getApprovalLink(approvedPayment);&#xD;
 &#xD;
